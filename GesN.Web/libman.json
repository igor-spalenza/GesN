{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/"
    },
    {
      "library": "jquery.qrcode@1.0",
      "destination": "wwwroot/lib/jquery.qrcode/"
    },
    {
      "library": "select2@4.0.13",
      "destination": "wwwroot/lib/select2/",
      "files": [
        "js/select2.min.js",
        "css/select2.min.css",
        "js/i18n/pt-br.min.js"
      ]
    },
    {
      "library": "jquery.mask@1.14.16",
      "destination": "wwwroot/lib/jquery.mask/",
      "files": [
        "jquery.mask.min.js"
      ]
    },
    {
      "library": "datatables.net@2.3.2",
      "destination": "wwwroot/lib/datatables.net/",
      "files": [
        "dataTables.min.js"
      ]
    },
    {
      "library": "datatables.net-bs5@2.3.2",
      "destination": "wwwroot/lib/datatables.net-bs5/",
      "files": [
        "dataTables.bootstrap5.min.js",
        "dataTables.bootstrap5.min.css"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "autocomplete.js@0.38.1",
      "destination": "wwwroot/lib/autocomplete.js/",
      "files": [
        "autocomplete.min.js",
        "autocomplete.js"
      ]
